{
  "name": "trading-queen-website",
  "version": "1.0.0",
  "description": "A comprehensive, dynamic, and responsive stock market website with real-time data, e-commerce functionality, professional financial services, and complete backend API.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "frontend": "http-server -p 8000 -c-1 -o",
    "backend": "nodemon server/server.js",
    "dev-full": "concurrently \"npm run backend\" \"npm run frontend\"",
    "install-deps": "npm install",
    "setup": "npm run install-deps && echo 'Setup complete! Run npm run dev for backend or npm run dev-full for full stack'",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o dist/css/style.min.css css/style.css",
    "minify-js": "uglifyjs js/*.js -o dist/js/bundle.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint js/*.js server/**/*.js",
    "format": "prettier --write \"**/*.{js,css,html}\"",
    "serve": "python -m http.server 8000",
    "deploy": "gh-pages -d .",
    "validate": "html-validate pages/*.html index.html"
  },
  "keywords": [
    "stock-market",
    "trading",
    "finance",
    "investment",
    "portfolio-management",
    "market-data",
    "real-time",
    "ecommerce",
    "financial-services",
    "responsive-design",
    "bootstrap",
    "javascript",
    "html5",
    "css3",
    "nodejs",
    "express",
    "api",
    "backend",
    "fii-dii-data",
    "institutional-data",
    "authentication",
    "jwt"
  ],
  "author": {
    "name": "Trading Queen",
    "email": "info@tradingqueen.com",
    "url": "https://tradingqueen.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tradingqueen/trading-queen-website.git"
  },
  "bugs": {
    "url": "https://github.com/tradingqueen/trading-queen-website/issues"
  },
  "homepage": "https://tradingqueen.com",
  "dependencies": {
    "bootstrap": "^5.3.0",
    "font-awesome": "^4.7.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.57.0",
    "gh-pages": "^6.1.1",
    "html-validate": "^8.7.4",
    "http-server": "^14.1.1",
    "prettier": "^3.1.1",
    "uglify-js": "^3.17.4",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "port": 3000,
    "host": "localhost",
    "frontend_port": 8000
  },
  "funding": {
    "type": "individual",
    "url": "https://tradingqueen.com/support"
  },
  "contributors": [
    {
      "name": "Trading Queen Team",
      "email": "dev@tradingqueen.com"
    }
  ],
  "files": [
    "index.html",
    "css/",
    "js/",
    "pages/",
    "server/",
    "assets/",
    "README.md"
  ],
  "directories": {
    "doc": "docs",
    "test": "tests",
    "lib": "server"
  },
  "private": false,
  "preferGlobal": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
